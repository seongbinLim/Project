<?xml version="1.0" encoding="UTF-8"?>
<!-- mappers/hrm.xml -->
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="airpollute"> <!-- mybatis는 namespace필수!! -->
	<select id="selectAll" resultType="com.ssafy.dto.AirPollution">
		SELECT *
		FROM airpollution
		ORDER BY pollutedate, district
	</select>
</mapper>



